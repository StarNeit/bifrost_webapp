{
  "name": "bifrost-web-app",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "aws codeartifact login --tool npm --repository bifrost --domain bifrost --domain-owner 701150199520 --namespace @xrite --region eu-west-1",
    "start": "npm run start-dev-server",
    "start-report": "webpack serve --config ./config/webpack.config.reports.dev.js",
    "start-dev-server": "webpack serve --config ./config/webpack.config.dev.js",
    "start-proxy": "node config/corsProxy.js",
    "start-formulation-ifs": "webpack-serve --config ./config/webpack.config.dev.js --open --ifsdb",
    "start:prod": "cross-env API_ENV=PROD NODE_ENV=production npm start",
    "start:uat": "cross-env API_ENV=UAT NODE_ENV=production npm start",
    "start:dev": "cross-env API_ENV=DEV NODE_ENV=development npm start",
    "dist:base": "rimraf dist && webpack --config ./config/webpack.config.dist.js",
    "dist:prod": "cross-env API_ENV=PROD NODE_ENV=production npm run dist:base --",
    "dist:uat": "cross-env API_ENV=UAT NODE_ENV=production npm run dist:base --",
    "dist:dev": "cross-env API_ENV=DEV NODE_ENV=development npm run dist:base --",
    "dist:local": "npm run dist:base --",
    "dist": "npm run dist:prod --",
    "dist-reports": "rimraf dist-reports && webpack --config ./config/webpack.config.reports.dist.js",
    "release": "np",
    "serve": "serve ./dist",
    "stats": "webpack --config ./config/webpack.config.stats.js",
    "start-mockup-server": "cd ./cypress/support/mockup && node ws.js",
    "test": "jest",
    "test:ui-start": "npm run preprocess && npm run --max-old-space-size=4096 cypress:run",
    "test:ui": "start-server-and-test start-mockup-server https://d27b7xf5gle4m7.cloudfront.net/ test:ui-start",
    "test:ui-dev-start": "npm run preprocess && npm run cypress:open",
    "test:ui-dev": "start-server-and-test start-mockup-server https://d27b7xf5gle4m7.cloudfront.net/ test:ui-dev-start",
    "test-report": "ts-node ./src/reports/report.integration.test.ts",
    "preprocess": "cd ./cypress && ts-node preprocess.ts",
    "cypress:email-notification": "ts-node ./cypress/generate-email-template.ts",
    "coverage": "jest --coverage",
    "cypress:run": "cypress run --config watchForFileChanges=false",
    "cypress:open": "cypress open --config experimentalInteractiveRunEvents=true",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint src && npm run type-check",
    "prepare": "husky install && rimraf .husky/.gitignore",
    "gen": "graphql-codegen --config codegen.yml",
    "gen-watch": "graphql-codegen --config codegen.yml --watch",
    "licenses-summary": "licenses-credits --checker --summary",
    "licenses-list-notAllowed": "licenses-credits --checker --exclude MIT,Apache,ISC,BSD,X-Rite,Unlicense,Apache-2.0,Zlib,Python-2.0,MPL-2.0,CC-BY-4.0,CC-BY-3.0,CC0-1.0,\"Public Domain\",WTFPL",
    "licenses-extract": "licenses-credits --checker --json --out licenses.json --customPath ./node_modules/@xrite/licenses-credits/licenseFormat.json",
    "merge-json-reports": "npx mochawesome-merge ./cypress/results/reports/mochawesome-json/*.json > ./cypress/results/reports/temp/testReport.json",
    "generate-html-report": "npx marge --reportDir './cypress/results/reports' --reportFilename 'bifrost_nightly_regression_report' --timestamp 'dd-mm-yyyy_HH:MM:ss' ./cypress/results/reports/temp/testReport.json --inline"
  },
